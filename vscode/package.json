{
  "name": "gitforme-vscode",
  "displayName": "GitForMe VSCode Extension",
  "description": "Analyze GitHub repositories and view insights from gitforme directly in VSCode.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:gitforme-sidebar"
  ],
  "main": "dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "gitforme",
          "title": "GitForMe",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "gitforme": [
        {
          "id": "gitforme-sidebar",
          "name": "GitForMe Insights",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p .",
    "watch": "tsc -watch -p ."
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "tslint": "^6.1.3",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  }
}
